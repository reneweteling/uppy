{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Uppy",
  "version": "1.0.6",
  "identifier": "com.weteling.uppy",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "uppy",
        "width": 800,
        "height": 600,
        "dragDropEnabled": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {},
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "signingIdentity": "-",
      "entitlements": null
    }
  }
}
